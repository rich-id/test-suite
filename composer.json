{
    "name": "richcongress/test-suite",
    "description": "The full RichCongress test suite for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "RichCongress",
            "homepage": "https://www.richcongress.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "richcongress/test-framework": "^0.2",
        "richcongress/test-tools": "^0.2",
        "richcongress/fixture-test": "^0.2",
        "richcongress/web-test-bundle": "^0.2",
        "richcongress/recurrent-fixtures-test-bundle": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "RichCongress\\TestSuite\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RichCongress\\TestSuite\\Tests\\": "Tests/"
        }
    },
    "config": {
        "discard-changes": true,
        "sort-packages": true
    }
}
